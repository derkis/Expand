.wrapper
	#start_game_prompt
	#game_invites
	= form_for @game, :remote => true do |game_f|
		= game_f.hidden_field :proposing_player, :value => current_user.id
		= game_f.fields_for :players, @game.players do |players_f|
			#online_user_table
				.table_title= 'Online Users'
			#notable_user_panel
				#checked_user_list.user_list
					= game_f.submit 'Send Invites', :disabled => !current_user.can_create_game?, :onclick => 'disable_button()'
				#friend_user_list.user_list
			- @online_users.each do |user|
				.user_cell{ :user_id => "#{user.id}" }
					= check_box_tag("game[players_attributes][#{user.id}][user_id]", user.id, false, :class => :user_checkbox)
					= players_f.label :user_id, user.email
			= players_f.hidden_field :user_id, :value => current_user.id	
