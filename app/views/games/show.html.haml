#game{ :game_id => @game.id, :player_index => @player_index }
	.debug_string= @game.current_turn.board
	.wrapper
		= render 'games/partials/players'
		= render 'games/partials/companies'
		= render 'games/partials/board'
		= button_to_function 'input', 'create_action_and_send_game_update()', :id => 'turn_button', :class => 'image_button'
		= button_to_function 'reset', 'reset_game()', :id => 'reset_game()'
